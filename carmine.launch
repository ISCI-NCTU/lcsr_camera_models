<launch>
  <arg name="sn"/>

  <!-- Include official launch file and specify camera_info urls -->
  <include file="$(find openni2_launch)/launch/openni2.launch">
    <!-- provide arguments to that launch file -->
    <arg name="depth_registration" value="true"/>
    <arg name="rgb_camera_info_url"
      value="package://lcsr_camera_models/cameras/carmine/calibration/rgb_$(arg sn).ini" />
    <arg name="depth_camera_info_url"
      value="package://lcsr_camera_models/cameras/carmine/calibration/depth_$(arg sn).ini" />
  </include>

</launch>




